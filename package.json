{
  "name": "@denofill/import-meta-main",
  "version": "0.1.0",
  "type": "module",
  "imports": {
    "#isMain.js": {
      "bun": "./src/isMain-default.js",
      "deno": "./src/isMain-default.js",
      "node": "./src/isMain-node.js",
      "browser": "./src/isMain-browser.js",
      "default": "./src/isMain-default.js"
    },
    "#isMainAsync.js": {
      "bun": "./src/isMainAsync-default.js",
      "deno": "./src/isMainAsync-default.js",
      "node": "./src/isMainAsync-node.js",
      "browser": "./src/isMainAsync-browser.js",
      "default": "./src/isMainAsync-default.js"
    }
  },
  "exports": {
    ".": "./src/polyfill.js",
    "./polyfill.js": "./src/polyfill.js",
    "./polyfillAsync.js": "./src/polyfillAsync.js",
    "./isMain.js": {
      "bun": "./src/isMain-default.js",
      "deno": "./src/isMain-default.js",
      "node": "./src/isMain-node.js",
      "browser": "./src/isMain-browser.js",
      "default": "./src/isMain-default.js"
    },
    "./isMainAsync.js": {
      "bun": "./src/isMainAsync-default.js",
      "deno": "./src/isMainAsync-default.js",
      "node": "./src/isMainAsync-node.js",
      "browser": "./src/isMainAsync-browser.js",
      "default": "./src/isMainAsync-default.js"
    }
  },
  "scripts": {
    "lint": "prettier --ignore-path .gitignore -w . && tsc --noEmit",
    "test": "npm run test:node",
    "test:browser": "serve",
    "test:node": "node --test"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "serve": "^14.2.0",
    "tinybench": "^2.5.0",
    "typescript": "^5.1.6"
  }
}
